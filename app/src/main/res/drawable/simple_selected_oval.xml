<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="@color/white" />

            <size
                android:width="20dp"
                android:height="20dp" />

            <solid android:color="@android:color/transparent" />
        </shape>
    </item>

    <item
        android:bottom="@dimen/oval_button_inner_margin"
        android:left="@dimen/oval_button_inner_margin"
        android:right="@dimen/oval_button_inner_margin"
        android:top="@dimen/oval_button_inner_margin">
        <shape android:shape="oval">

            <size
                android:width="20dp"
                android:height="20dp" />

            <solid android:color="@color/white" />

        </shape>

    </item>

</layer-list>

